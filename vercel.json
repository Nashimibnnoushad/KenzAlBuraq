{
    "functions": {
        "assets/inc/php/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        { "src": "/assets/inc/php/(.*)", "dest": "/assets/inc/php/$1" },
        { "src": "/public/(.*)", "headers": { "cache-control": "s-maxage=31536000, stale-while-revalidate" } },
        { "src": "/(.*)", "dest": "index.html" }
    ]
}